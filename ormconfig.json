{
   "type": "postgres",
   "host": "localhost",
   "port": 5432,
   "database": "dc-snippets",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/**/*.entity.{ts,js}"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}